                        android:layout_alignStart="@+id/view_right_front"
                        android:layout_alignTop="@+id/view_right_front"
                        android:layout_alignEnd="@+id/view_right_front"
                        android:layout_alignBottom="@+id/view_right_front"
                        android:text="@string/right_front" />

                    <TextView
                        android:id="@+id/text_view_bump1_rf"
                        style="@style/ThirdTitleTextView"
                        android:layout_below="@+id/view_right_front"
                        android:layout_alignParentStart="true"
                        android:text="@string/bump" />

                    <ProgressBar
                        android:id="@+id/progress_bar_bump_rf"
                        style="@style/ProgressBar"
                        android:layout_alignTop="@+id/text_view_bump1_rf"
                        android:layout_alignBottom="@+id/text_view_bump1_rf"
                        android:theme="@style/ProgressBar" />

                    <TextView
                        android:id="@+id/text_view_bump_rf"
                        style="@style/TextViewProgressBar"
                        android:layout_alignStart="@id/progress_bar_bump_rf"
                        android:layout_alignTop="@id/progress_bar_bump_rf"
                        android:layout_alignEnd="@id/progress_bar_bump_rf"
                        android:layout_alignBottom="@id/progress_bar_bump_rf" />

                    <TextView
                        android:id="@+id/text_view_fast_bump1_rf"
