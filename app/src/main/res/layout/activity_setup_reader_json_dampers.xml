                        android:layout_alignStart="@id/progress_bar_rebound_lr"
                        android:layout_alignTop="@id/progress_bar_rebound_lr"
                        android:layout_alignEnd="@id/progress_bar_rebound_lr"
                        android:layout_alignBottom="@id/progress_bar_rebound_lr" />

