                    <TextView
                        android:id="@+id/text_view_fast_rebound_lf"
                        style="@style/TextViewProgressBar"
                        android:layout_alignStart="@id/progress_bar_fast_rebound_lf"
                        android:layout_alignTop="@id/progress_bar_fast_rebound_lf"
                        android:layout_alignEnd="@id/progress_bar_fast_rebound_lf"
                        android:layout_alignBottom="@id/progress_bar_fast_rebound_lf" />

