                        style="@style/SecondTitleTextView"
                        android:layout_alignStart="@+id/view_left_front"
                        android:layout_alignTop="@+id/view_left_front"
                        android:layout_alignEnd="@+id/view_left_front"
                        android:layout_alignBottom="@+id/view_left_front"
                        android:text="@string/left_front" />

                    <TextView
                        android:id="@+id/text_view_bump1_lf"
                        style="@style/ThirdTitleTextView"
                        android:layout_below="@+id/view_left_front"
                        android:text="@string/bump" />

                    <ProgressBar
                        android:id="@+id/progress_bar_bump_lf"
                        style="@style/ProgressBar"
                        android:layout_alignTop="@+id/text_view_bump1_lf"
                        android:layout_alignBottom="@+id/text_view_bump1_lf"
                        android:theme="@style/ProgressBar" />

                    <TextView
                        android:id="@+id/text_view_bump_lf"
                        style="@style/TextViewProgressBar"
                        android:layout_alignStart="@id/progress_bar_bump_lf"
                        android:layout_alignTop="@id/progress_bar_bump_lf"
                        android:layout_alignEnd="@id/progress_bar_bump_lf"
                        android:layout_alignBottom="@id/progress_bar_bump_lf" />

                    <TextView
                        android:id="@+id/text_view_fast_bump1_lf"
                        style="@style/ThirdTitleTextView"
                        android:layout_below="@+id/text_view_bump1_lf"
                        android:text="@string/fast_bump" />

                    <ProgressBar
                        android:id="@+id/progress_bar_fast_bump_lf"
                        style="@style/ProgressBar"
                        android:layout_alignTop="@+id/text_view_fast_bump1_lf"
                        android:layout_alignBottom="@+id/text_view_fast_bump1_lf"
                        android:theme="@style/ProgressBar" />

                    <TextView
                        android:id="@+id/text_view_fast_bump_lf"
                        style="@style/TextViewProgressBar"
                        android:layout_alignStart="@id/progress_bar_fast_bump_lf"
                        android:layout_alignTop="@id/progress_bar_fast_bump_lf"
                        android:layout_alignEnd="@id/progress_bar_fast_bump_lf"
                        android:layout_alignBottom="@id/progress_bar_fast_bump_lf" />

                    <TextView
                        android:id="@+id/text_view_rebound1_lf"
