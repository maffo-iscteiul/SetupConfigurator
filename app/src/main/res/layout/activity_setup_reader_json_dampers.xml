                        android:layout_alignStart="@id/progress_bar_fast_rebound_lr"
                        android:layout_alignTop="@id/progress_bar_fast_rebound_lr"
                        android:layout_alignEnd="@id/progress_bar_fast_rebound_lr"
                        android:layout_alignBottom="@id/progress_bar_fast_rebound_lr" />

                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="5dp"
                    android:background="@drawable/table_background"
                    android:paddingBottom="5dp">

                    <View
                        android:id="@+id/view_right_rear"
                        style="@style/SecondTitleView" />

                    <TextView
                        android:id="@+id/text_view_right_rear"
                        style="@style/SecondTitleTextView"
                        android:layout_alignStart="@+id/view_right_rear"
                        android:layout_alignTop="@+id/view_right_rear"
                        android:layout_alignEnd="@+id/view_right_rear"
                        android:layout_alignBottom="@+id/view_right_rear"
                        android:text="@string/right_rear" />

                    <TextView
                        android:id="@+id/text_view_bump1_rr"
                        style="@style/ThirdTitleTextView"
                        android:layout_below="@+id/view_right_rear"
                        android:text="@string/bump" />

                    <ProgressBar
                        android:id="@+id/progress_bar_bump_rr"
                        style="@style/ProgressBar"
                        android:layout_alignTop="@+id/text_view_bump1_rr"
                        android:layout_alignBottom="@+id/text_view_bump1_rr"
                        android:theme="@style/ProgressBar" />

                    <TextView
                        android:id="@+id/text_view_bump_rr"
                        style="@style/TextViewProgressBar"
                        android:layout_alignStart="@id/progress_bar_bump_rr"
                        android:layout_alignTop="@id/progress_bar_bump_rr"
                        android:layout_alignEnd="@id/progress_bar_bump_rr"
                        android:layout_alignBottom="@id/progress_bar_bump_rr" />

                    <TextView
                        android:id="@+id/text_view_fast_bump1_rr"
                        style="@style/ThirdTitleTextView"
                        android:layout_below="@+id/text_view_bump1_rr"
                        android:layout_alignParentStart="true"
                        android:text="@string/fast_bump" />

                    <ProgressBar
                        android:id="@+id/progress_bar_fast_bump_rr"
                        style="@style/ProgressBar"
                        android:layout_alignTop="@+id/text_view_fast_bump1_rr"
                        android:layout_alignBottom="@+id/text_view_fast_bump1_rr"
                        android:theme="@style/ProgressBar" />

                    <TextView
                        android:id="@+id/text_view_fast_bump_rr"
                        style="@style/TextViewProgressBar"
                        android:layout_alignStart="@id/progress_bar_fast_bump_rr"
                        android:layout_alignTop="@id/progress_bar_fast_bump_rr"
                        android:layout_alignEnd="@id/progress_bar_fast_bump_rr"
                        android:layout_alignBottom="@id/progress_bar_fast_bump_rr" />

                    <TextView
                        android:id="@+id/text_view_rebound1_rr"
                        style="@style/ThirdTitleTextView"
                        android:layout_below="@+id/text_view_fast_bump1_rr"
                        android:text="@string/rebound" />

                    <ProgressBar
                        android:id="@+id/progress_bar_rebound_rr"
                        style="@style/ProgressBar"
                        android:layout_alignTop="@+id/text_view_rebound1_rr"
                        android:layout_alignBottom="@+id/text_view_rebound1_rr"
                        android:theme="@style/ProgressBar" />

                    <TextView
                        android:id="@+id/text_view_rebound_rr"
                        style="@style/TextViewProgressBar"
                        android:layout_alignStart="@id/progress_bar_rebound_rr"
                        android:layout_alignTop="@id/progress_bar_rebound_rr"
                        android:layout_alignEnd="@id/progress_bar_rebound_rr"
