                        android:layout_alignStart="@id/progress_bar_rebound_lf"
                        android:layout_alignTop="@id/progress_bar_rebound_lf"
                        android:layout_alignEnd="@id/progress_bar_rebound_lf"
                        android:layout_alignBottom="@id/progress_bar_rebound_lf" />

                    <TextView
