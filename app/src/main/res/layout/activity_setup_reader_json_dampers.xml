                        style="@style/ProgressBar"
                        android:layout_alignTop="@+id/text_view_fast_bump1_lr"
                        android:layout_alignBottom="@+id/text_view_fast_bump1_lr"
                        android:theme="@style/ProgressBar" />

                    <TextView
                        android:id="@+id/text_view_fast_bump_lr"
                        style="@style/TextViewProgressBar"
                        android:layout_alignStart="@id/progress_bar_fast_bump_lr"
                        android:layout_alignTop="@id/progress_bar_fast_bump_lr"
